#include<iostream>
using namespace std;
Class Room{
     public:
     double length;
     double breadth;
     double height;
     double calculateArea() {
       return length*breadth;
    }
    double calculatevolume() {
       return length*breadth*height;
    }
};
int main() {
    Room room1,room2;
    room1.length = 42.5;
    room1.breadth = 30.8;
    room1.height = 19.2;
    cout<< "Area of Room = " << room1.calculateArea() << endl;
    cout<< "Volume of Room = " << room2.calculatevolume() << endl;
